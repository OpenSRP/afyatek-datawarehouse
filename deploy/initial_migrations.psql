--
-- Name: create_new_tables(); Type: FUNCTION; Schema: public; Owner: dtreedev
--

CREATE FUNCTION public.create_new_tables() RETURNS void
    LANGUAGE plpgsql
    AS $_$

    DECLARE event_name TEXT;

    BEGIN

        FOR event_name IN (
            SELECT DISTINCT(
                data::jsonb ->>'eventType') 
            FROM 
                raw_data.raw_events
            EXCEPT 
            SELECT 
                event_type 
            FROM 
                db_state.existing_tables
            )
        LOOP
            EXECUTE FORMAT(' CREATE TABLE IF NOT EXISTS public.%1$s('
                'id VARCHAR(36) NOT NULL UNIQUE,'
                'base_entity_id VARCHAR NOT NULL,'
                'entity_type VARCHAR NOT NULL,'
                'event_date TIMESTAMP NOT NULL,'
                'event_type VARCHAR NOT NULL,'
                'team VARCHAR NOT NULL,'
                'location_id VARCHAR NOT NULL,'
                'child_location_id VARCHAR NOT NULL,'
                'provider_id VARCHAR NOT NULL,'
                'server_version BIGINT NOT NULL DEFAULT 0,'
                'date_created TIMESTAMP NOT NULL,'
                'obs JSONB NOT NULL,'
                'PRIMARY KEY (id));'
                'INSERT INTO db_state.existing_tables '
                '(event_type, table_name) VALUES '
                '(''%2$s'', LOWER(''%1$s''));', 
                REGEXP_REPLACE(event_name,'[^\w]+', '_','g'), 
                event_name);
        END LOOP;
        RETURN;

    END;
    $_$;


ALTER FUNCTION public.create_new_tables() OWNER TO :db_user;

